{
  "functions": {
    "api/*.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/docs/index.html"
    },
    {
      "source": "/login",
      "destination": "/docs/login.html"
    },
    {
      "source": "/sources",
      "destination": "/docs/sources.html"
    },
    {
      "source": "/users",
      "destination": "/docs/users.html"
    }
  ]
}